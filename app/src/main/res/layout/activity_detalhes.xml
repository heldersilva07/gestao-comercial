<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.heldersilva.comercialgestao.DetalhesActivity"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/lblnome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="82dp"
        android:text="Nome"
        app:layout_constraintStart_toStartOf="@+id/lblmorada"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="82dp" />

    <TextView
        android:id="@+id/lblmorada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="20dp"
        android:text="Morada"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/lblnome"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="121dp" />

    <TextView
        android:id="@+id/lblnumero"
        android:layout_width="67dp"
        android:layout_height="23dp"
        android:layout_marginTop="13dp"
        android:text="Telefone"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/lblmorada"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="153dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:text="Nome:"
        app:layout_constraintBaseline_toBaselineOf="@+id/lblnome"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="82dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Morada:"
        app:layout_constraintBaseline_toBaselineOf="@+id/lblmorada"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="121dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefone:"
        app:layout_constraintBaseline_toBaselineOf="@+id/lblnumero"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="153dp" />


</android.support.constraint.ConstraintLayout>
